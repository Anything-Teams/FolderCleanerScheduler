{
  "name": "scheduler",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.greendaero.scheduler",
    "productName": "folder-cleaner-scheduler",
    "files": [
      "**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "assets/greendaero.ico",
      "requestedExecutionLevel": "requireAdministrator"
    }
  },
  "devDependencies": {
    "electron": "^24.0.0",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "iconv-lite": "^0.6.3"
  }
}
